package DataTypesandVariables_Lab_2;
import java.util.Scanner;
public class CenturiesToMinutes_9 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        Scanner scanner = new Scanner(System.in);
        double centuries = in.nextDouble();
        double years = centuries * 100;
        double days = years * 365.2422;
        double hours = days * 24.00;
        double minutes = hours * 60.00;
        System.out.printf("%.0f centuries = %.0f years = %.0f days = %.0f hours = %.0f minutes%n", centuries, years, days, hours, minutes);
    }
}
